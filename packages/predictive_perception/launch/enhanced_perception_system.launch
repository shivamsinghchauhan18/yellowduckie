<?xml version="1.0" encoding="utf-8"?>
<launch>
    <arg name="veh" default="$(env VEHICLE_NAME)" />
    <arg name="config" default="default" />
    
    <!-- Enhanced Object Detection -->
    <include file="$(find object_detection)/launch/enhanced_object_detection_node.launch">
        <arg name="veh" value="$(arg veh)" />
        <arg name="config" value="$(arg config)" />
    </include>
    
    <!-- Predictive Perception Manager -->
    <include file="$(find predictive_perception)/launch/predictive_perception_manager_node.launch">
        <arg name="veh" value="$(arg veh)" />
        <arg name="config" value="$(arg config)" />
    </include>
    
    <!-- Scene Understanding Module -->
    <include file="$(find predictive_perception)/launch/scene_understanding_module_node.launch">
        <arg name="veh" value="$(arg veh)" />
        <arg name="config" value="$(arg config)" />
    </include>
    
    <!-- Perception Data Fusion -->
    <include file="$(find predictive_perception)/launch/perception_data_fusion_node.launch">
        <arg name="veh" value="$(arg veh)" />
        <arg name="config" value="$(arg config)" />
    </include>
</launch>