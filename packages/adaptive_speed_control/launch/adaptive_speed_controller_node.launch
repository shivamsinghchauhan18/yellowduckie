<!-- packages/adaptive_speed_control/launch/adaptive_speed_controlled.launch -->
<launch>
  <arg name="veh" default="$(env VEHICLE_NAME)"/>
  <arg name="config" default="baseline"/>
  <arg name="param_file_name" default="default"/>
  <arg name="verbose" default="false"/>

  <include file="$(find duckietown_demos)/launch/master.launch">
    <arg name="veh" value="$(arg veh)"/>
    <arg name="config" value="$(arg config)"/>
    <arg name="param_file_name" value="$(arg param_file_name)"/>
    <!-- Let master launch the adaptive node to avoid duplicates -->
    <arg name="use_adaptive_speed" value="true"/>
    <!-- pass other args you already use here (e.g., use_ml_lane, onnx_model_path, roboflow toggles, etc.) -->
  </include>
</launch>